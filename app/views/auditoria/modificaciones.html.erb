<%= render "partials/navigation" %>

<!-- Page Content -->
<div id="page-wrapper">
	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">Ultimas Modificaciones </h1>
				<div class="panel panel-default">
					<div class="panel-heading">
						Accesos
					</div>
					<!-- /.panel-heading -->
					<div class="panel-body">
						<div class="table-responsive">
							<table class="table">
								<thead>
									<tr>
										<th>Id</th>
										<th>Usuario</th>
										<th>Accion</th>
										<th>Valor Viejo</th>
										<th>Valor Nuevo</th>
										<th>Fecha y Hora</th>
									</tr>
								</thead>
								<tbody>
									<% @auditoria.each do |item| %>
									<tr>
										<td><%= item.id %></td>
										<td><%= item.usuario %></td>
                                        <td><%= item.accion %></td>
										<td><%= item.valor_viejo %></td>
										<td><%= item.valor_nuevo %></td>
										<td><%= item.created_at %></td>
									</tr>
									<% end %>
								</tbody>
							</table>
						</div>
						<!-- /.table-responsive -->
					</div>
					<!-- /.panel-body -->
				</div>
				<!-- /.panel -->
			</div>
			<!-- /.col-lg-12 -->
		</div>
		<!-- /.row -->
	</div>
	<!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->