<p id="notice"><%= notice %></p>

<h1>Listing Tipo Canchas</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_canchas.each do |tipo_cancha| %>
      <tr>
        <td><%= tipo_cancha.descripcion %></td>
        <td><%= link_to 'Show', tipo_cancha %></td>
        <td><%= link_to 'Edit', edit_tipo_cancha_path(tipo_cancha) %></td>
        <td><%= link_to 'Destroy', tipo_cancha, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo cancha', new_tipo_cancha_path %>
